import java.util.*;
 

class Main{
  public static void main(String[] args)
  {
      Scanner sc=new Scanner(System.in);
      int T=sc.nextInt();
      while(T-->0)
      {
          int N=sc.nextInt();
          int sum=0;
          int m=N;
          while(m!=0){
              int rem=m%10;
              sum=sum*10+rem;
              m=m/10;
          }
          System.out.println(sum);
      }
          
      }
  }
